{
  "name": "connect-share",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "lint:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint --fix **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && yarn run lint:style",
    "test": "npm run test:unit && yarn run test:e2e",
    "test:unit": "cross-env TEST=unit ava ./test/specs/**/* --timeout=5m",
    "test:e2e": "cross-env TEST=e2e ava ./test/e2e/**/* --timeout=5m"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix",
    "*.{css,vue}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/toast": "^3.3.1",
    "@nuxtjs/vuetify": "^1.11.3",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.27.0",
    "emailjs-com": "^2.6.4",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.19",
    "multer": "^1.4.2",
    "nuxt": "^2.15.2",
    "socket.io": "^3.1.2",
    "socket.io-client": "^3.1.2",
    "uuid": "^8.3.2",
    "vue-chat-scroll": "^1.4.0",
    "vue-youtube": "^1.4.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/test-utils": "^1.1.3",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  }
}
